<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4"
	xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>NodeClient</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>DispatcherServlet</servlet-name>
		<servlet-class>node.http.DispatcherServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>DispatcherServlet</servlet-name>
		<url-pattern>/dispatch</url-pattern>
	</servlet-mapping>
	
	<listener>
    	<description>ServletContextListener</description>
    	<listener-class>node.ContextListener</listener-class>
	</listener>
	
	
	<context-param> 
    	<param-name>libLocation</param-name> 
    	<param-value>@nodeClient.tasklibs@</param-value> 
	</context-param> 

	<context-param> 
    	<param-name>confLocation</param-name>
    	<param-value>@nodeClient.taskconfigs@</param-value> 
	</context-param> 
	
	
	<context-param> 
    	<param-name>sshUser</param-name> 
    	<param-value>@nodeClient.sshUsr@</param-value> 
	</context-param> 

	<context-param> 
    	<param-name>sshPassword</param-name>
    	<param-value>@nodeClient.sshPassword@</param-value> 
	</context-param> 
	
</web-app>
